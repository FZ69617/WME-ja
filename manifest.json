{
  "name": "WME-JunctionAngle",
  "version": "1.5.4",
  "manifest_version": 2,

  "description": "This extension adds junction angle information the the Waze map editor.",
  "icons": { "16": "ja16.png",
             "48": "ja48.png",
             "128": "ja128.png"
           },

  "homepage_url": "https://github.com/milkboy/WME-ja",

  "content_scripts": [
    {
      "matches": [
        "https://world.waze.com/editor/*",
        "https://waze.com/editor/*",
        "https://www.waze.com/editor/*",
        "https://world.waze.com/beta_editor/*",
        "https://waze.com/beta_editor/*",
        "https://www.waze.com/beta_editor/*",
        "https://editor.waze.com/*",
        "https://editor-beta.waze.com/*"
      ],
      "js": ["crx_bootstrap.user.js"]
    }
  ],
  "web_accessible_resources": ["wme_junctionangle.user.js"]
}
